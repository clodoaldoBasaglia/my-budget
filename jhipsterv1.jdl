


entity BankAccount {
	descriptionName String,
    accountNumber String,
    accountDigit String,
    agencyNumber String,
    agencyDigit String
}


relationship OneToMany{
	Bank to BankAccount{bank}
}

entity Bank {
	descriptionName String,
    bankType BankTypeEnum
}

enum BankTypeEnum {
	DIGITAL, FISICO
}

enum BillTypeEnum{
	MENSAL, UNICA
}

enum PaymentScheduleEnum{
	PARCELADO, UNICA
}

enum PaymentTypeEnum {
	PIX, CREDITO, BOLETO, DEBITO
}